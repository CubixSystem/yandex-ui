@define-mixin Button_filled-next
    $viewName,
    $colorBase,
    $fillColorBase,
    $colorDisabled,
    $fillColorDisabled,
    $colorChecked,
    $fillColorChecked {
    /* common */
    @mixin Button_base-next
        $viewName,
        $colorBase,
        $fillColorBase;
    /* press */
    @mixin Button_pressed-next
        $viewName;

    /* disable */
    @mixin Button_disabled-next
        $viewName,
        $colorDisabled,
        $fillColorDisabled;

    /* check */
    @mixin Button_checked-next
        $viewName,
        $colorChecked,
        $fillColorChecked;
}
